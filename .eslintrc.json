{
  "env": {
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-undef": "off"
  },
  "globals": {
    "__ENV": "readonly",
    "__ITER": "readonly",
    "__VU": "readonly"
  }
}
